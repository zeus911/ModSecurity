{
    "name": "phpModSecurity/ModSecurity",
    "description": "PHP port of ModSecurity",
    "keywords": [
        "league",
        "ModSecurity"
    ],
    "homepage": "https://github.com/phpModSecurity/ModSecurity",
    "license": "MIT",
    "authors": [
        {
            "name": "Albert Moreno",
            "email": "albert.moreno.t@gmail.com",
            "homepage": "https://github.com/torrentalle",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : ">=5.3.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*",
        "scrutinizer/ocular": "~1.1"
    },
    "autoload": {
        "psr-4": {
            "phpModSecurity\\ModSecurity\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "phpModSecurity\\ModSecurity\\Test\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
